# Copy this file to .env and fill in your values.
# Then run: source .env
# This file should be added to .gitignore and never committed.

# Your GCP project ID (found in the GCP Console dashboard)
export TF_VAR_project_id="your-gcp-project-id"

# A globally unique suffix for the Cloud Storage bucket name.
# Use something like your initials + today's date, e.g. "ab-20240301"
export TF_VAR_bucket_suffix="your-unique-suffix"

# Path to your GCP service account key file
export GOOGLE_APPLICATION_CREDENTIALS="/path/to/your-key-file.json"
